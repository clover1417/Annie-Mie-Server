# Server Connection (deployment-specific)
HOST=localhost
PORT=8765

# FFmpeg Path (platform-specific, Windows only)
# Uncomment and set if FFmpeg is not in PATH
# FFMPEG_PATH=C:\ffmpeg\bin

# vLLM Configuration
# GPU memory fraction to use (0.0-1.0)
VLLM_GPU_MEMORY_UTILIZATION=0.95
# Maximum context length
VLLM_MAX_MODEL_LEN=32768
# Maximum concurrent sequences for batch processing
VLLM_MAX_NUM_SEQS=8
# Maximum multimodal items per prompt (images, videos, audio)
VLLM_LIMIT_MM_PER_PROMPT=3
